/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or(a=a,b=b,out=or);

    And(a=a,b=b,out=and);
    Not(in=and, out=outnot);
    And(a=or, b=outnot, out=out);

}
